/*
 * Automatic software update for U-Boot
 */

/dts-v1/;

/ {
	description = "Upgrade Image";
	#address-cells = <1>;

	images {
		script@1 {
			data = /incbin/("out.scr");
			type = "script";
			load = "0x00408000";
			entry = "0x00408058";
			compression = "none";
			os = "linux";
			arch = "arm";
			signature@1 {
				algo = "sha1,rsa2048";
				key-name-hint = "dev";
			};
		};
	};
};
